<odoo>
    <data>


        <record id="stock_valuation_layer_tree_ext_haval_date_customization" model="ir.ui.view">
          <field name="name">stock.valuation.layer.tree.ext.haval.date.customization</field>
          <field name="model">stock.valuation.layer</field>
          <field name="inherit_id" ref="stock_account.stock_valuation_layer_tree" />
          <field name="arch" type="xml">
            <xpath expr="//field[@name='create_date']" position="replace">

              <field name="historical_date" string="Date"/>
              
            </xpath>
          </field>
        </record>


    </data>
</odoo>